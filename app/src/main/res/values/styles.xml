<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="FaceButtonStyle" parent="Base.Widget.AppCompat.Button">
        <item name="colorButtonNormal">@color/blue_face</item>
        <item name="android:textColor">@color/white</item>
    </style>
    #F79521
    <style name="AccentButtonStyle" parent="Base.Widget.AppCompat.Button">
        <item name="colorButtonNormal">@color/colorAccent</item>
        <item name="android:textColor">@color/white</item>

    </style>

    <style name="AccentButtonStyles" parent="Base.Widget.AppCompat.Button">
        <item name="colorButtonNormal">@color/colorAccents</item>
        <item name="android:textColor">@color/white</item>

    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@color/white</item>
    </style>

    <style name="MSBottomNavigation">
        <item name="android:layout_height">@dimen/ms_bottom_navigation_bar_height</item>
        <item name="android:background">@color/ms_bottomNavigationBackgroundColor</item>
    </style>


</resources>
